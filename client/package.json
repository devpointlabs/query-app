{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "devise-axios": "^1.0.24",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "2.1.3",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.87.2",
    "styled-components": "^4.3.2"
  },
  "proxy": "http://localhost:3001",
